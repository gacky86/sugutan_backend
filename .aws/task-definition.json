{
  "containerDefinitions": [
    {
      "name": "sugutan-api",
      "image": "178795222462.dkr.ecr.ap-northeast-1.amazonaws.com/prd-sugutan-api:latest",
      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "RAILS_LOG_TO_STDOUT",
          "value": "true"
        },
        {
          "name": "RAILS_ENV",
          "value": "production"
        },
        {
          "name": "RAILS_SERVE_STATIC_FILES",
          "value": "true"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "secrets": [
        {
          "name": "DB_HOST",
          "valueFrom": "arn:aws:ssm:ap-northeast-1:178795222462:parameter/sugutan-api/prd/rds/db_host"
        },
        {
          "name": "DB_NAME",
          "valueFrom": "arn:aws:ssm:ap-northeast-1:178795222462:parameter/sugutan-api/prd/rds/db_name"
        },
        {
          "name": "DB_USERNAME",
          "valueFrom": "arn:aws:ssm:ap-northeast-1:178795222462:parameter/sugutan-api/prd/rds/db_username"
        },
        {
          "name": "DB_PASSWORD",
          "valueFrom": "arn:aws:ssm:ap-northeast-1:178795222462:parameter/sugutan-api/prd/rds/db_password"
        },
        {
          "name": "RAILS_MASTER_KEY",
          "valueFrom": "arn:aws:ssm:ap-northeast-1:178795222462:parameter/sugutan-api/prd/rds/rails_master_key"
        },
        {
          "name": "SMTP_USERNAME",
          "valueFrom": "arn:aws:ssm:ap-northeast-1:178795222462:parameter/sugutan-api/prd/smtp/smtp_username"
        },
        {
          "name": "SMTP_PASSWORD",
          "valueFrom": "arn:aws:ssm:ap-northeast-1:178795222462:parameter/sugutan-api/prd/smtp/smtp_password"
        },
        {
          "name": "GEMINI_API_KEY",
          "valueFrom": "arn:aws:ssm:ap-northeast-1:178795222462:parameter/sugutan-api/prd/gemini/gemini_api_key"
        },
        {
          "name": "GMAIL_USER_NAME",
          "valueFrom": "arn:aws:ssm:ap-northeast-1:178795222462:parameter/sugutan-api/prd/gmail/gmail_user_name"
        },
        {
          "name": "GMAIL_PASSWORD",
          "valueFrom": "arn:aws:ssm:ap-northeast-1:178795222462:parameter/sugutan-api/prd/gmail/gmail_password"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/prd-sugutan-api",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "sugutan-api"
        }
      },
      "systemControls": []
    }
  ],
  "family": "prd-sugutan-api",
  "taskRoleArn": "arn:aws:iam::178795222462:role/prd-sugutan-api-task-role",
  "executionRoleArn": "arn:aws:iam::178795222462:role/prd-sugutan-api-execution-role",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}
